{ "routes": [{ "src": "/[^.]+", "dest": "/", "status": 200 }] }
{
    "version": 2,
    "public": true,
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
      {
        "source": "/(.*).(mp3|wav)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }